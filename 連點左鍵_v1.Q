//**** Delay
UserVar gMoveDelay = 10 "Move Delay(ms)"
UserVar gClockDelay = 50 "Click Delay(ms)"

//******** Parameters
Dim cTrue
cTrue = 1
Dim cFalse
cFalse = 0
//**** Coordinate
gClockXY = array(900, 650)

Function mainFunction()
	While (1)
		//** Click 
		MoveTo gClockXY(0), gClockXY(1)
		Delay gMoveDelay
		LeftClick 1
		Delay gClockDelay
	Wend
End Function


Call mainFunction()
