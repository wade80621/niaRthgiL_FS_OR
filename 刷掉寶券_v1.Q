Function combo2Keys(ukey1, ukey2)
	KeyDown ukey1, 1
	KeyPress ukey2, 1
	KeyUp ukey1, 1
End Function

Function keyLoops(uKey, uLoops, uDelay)
	For uLoops
		Call Plugin.Bkgnd.KeyPress(hwnd, uKey)
		Delay uDelay*gDelayMultiple
	Next
End Function

Function dropAtoB(uAX, uAY, uBX, uBY)
	MoveTo uAX, uAY
	Delay 250*gDelayMultiple
	LeftDown 1
	Delay 250*gDelayMultiple
	MoveTo uBX, uBY
	Delay 350*gDelayMultiple
	LeftUp 1
	Delay 250*gDelayMultiple
End Function

Function findCoordinateNum(uLUX, uLUY, uRDX, uRDY, uNum)
	Dim uWBreak
	Dim uTargetString
	Dim uResult
	uResult = cFalse
	uNum = CStr(uNum)
	uTargetString = "Attachment:\"+uNum+".bmp"
	
	uWBreak=gVCodeNumTryCount
	While(uWBreak>0)
		FindPic uLUX, uLUY, uRDX, uRDY, uTargetString, 1, intX, intY
		If intX > 0 And intY > 0 Then
    		TracePrint "Find: "+uNum+".bmp"
    		uWBreak = 0
    		uResult = cTrue
    	// Else 
    	//	TracePrint "Not Found: "+uNum+".bmp"
		End If
		Delay 25
		uWBreak = uWBreak - 1
	Wend
	findCoordinateNum = uResult
End Function

Function enterName(uStringLen, uarString)
	Dim uIndex
	uIndex=0
	While(uIndex<uStringLen)
		KeyPress uarString(uIndex), 1
		uIndex = uIndex + 1
		Delay 25*gDelayMultiple
	Wend
End Function

Function mainFunction()
	Dim uarName
	Dim uStringLen
	
	If (gCheckSetting <> 46804706) Then 
		TracePrint "WARNING: ÀË¬d½X¤£¥¿½T"
		ExitScript
	End If

	// ** Create character
	// Select Slot
	MoveTo gCharacterSlotX, gCharacterSlotY
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 250 * gDelayMultiple
	// Enter Name
	uarName = array("P", "O", "E", "B", "O", "R", "I", "N", "G", "-")
	uStringLen = 10
	Call enterName(uStringLen, uarName)
	Delay 100*gDelayMultiple
	Call Plugin.Bkgnd.KeyPress(hwnd, Asc(gCharacterMark))
	Delay 100 * gDelayMultiple
	// Create
	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
	Delay 100 * gDelayMultiple
	// EnterGame
	MoveTo gCharacterSlotX, gCharacterSlotY
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 100 * gDelayMultiple
	Call Plugin.Bkgnd.KeyPress(hwnd, 13)
	Delay 3000 * gDelayMultiple
	// Create character **
	
	// ** Go to City
	// Enter Castle
	MoveTo gMap1XY(0), gMap1XY(1)
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 100*gDelayMultiple
	LeftClick 1
	Delay 5000 * gDelayMultiple
	
	// Move To NPC
	MoveTo gMap2XY(0), gMap2XY(1)
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 3000 * gDelayMultiple
	
	// Right Click NPC
	MoveTo gNewNPCXY(0), gNewNPCXY(1)
	Delay 25*gDelayMultiple
	RightClick 1
	Delay 250 * gDelayMultiple
	
	// Transfer to City
	Call keyLoops(13, 6, 500)
	Delay 5000 * gDelayMultiple
	// Go to City **
	
	// ** Store
	// Open Store House
	MoveTo gCapraXY(0), gCapraXY(1)
	Delay 25*gDelayMultiple
	RightClick 1
	Delay 250 * gDelayMultiple
	Call keyLoops(13, 2, 500)
	
	// Open Bag
	Call combo2Keys("Alt", "E")
	Delay 250 * gDelayMultiple
	// MoveTo gCharManuXY(0), gCharManuXY(1)
	// Delay 25*gDelayMultiple
	// LeftClick 1
	// Delay 100 * gDelayMultiple
	// MoveTo gOpenBagXY(0), gOpenBagXY(1)
	// Delay 25*gDelayMultiple
	// LeftClick 1
	// Delay 100 * gDelayMultiple


	// Switch to Bag page 1
	MoveTo gBagPage1XY(0), gBagPage1XY(1)
	Delay 25 * gDelayMultiple
	LeftClick 1
	Delay 300 * gDelayMultiple
	
	// Store Item * 2
	For (2)
		Call dropAtoB(gBagItem2XY(0), gBagItem2XY(1), gStorehouseXY(0), gStorehouseXY(1))
		Call Plugin.Bkgnd.KeyPress(hwnd, 13)
		Delay 100 * gDelayMultiple
	Next
	
	Delay 500 * gDelayMultiple
			
	// Close Store House
	MoveTo gStorehouseCloseXY(0), gStorehouseCloseXY(1)
	Delay 25 * gDelayMultiple
	LeftClick 1
	Delay 500 * gDelayMultiple
	// Store **
	
	// ** Delete Character
	// Back to Character select
	Call keyLoops(27, 2, 100)
	Delay 100 * gDelayMultiple
	MoveTo gBackToCharSelXY(0), gBackToCharSelXY(1)
	Delay 25 * gDelayMultiple
	LeftClick 1
	Delay 4000 * gDelayMultiple

	// Select Slot
	MoveTo gCharacterSlotX, gCharacterSlotY
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 250 * gDelayMultiple
	
	// Delete 1
	MoveTo gDeleteChar1XY(0), gDeleteChar1XY(1)
	Delay 25*gDelayMultiple
	LeftClick 1
	Delay 250 * gDelayMultiple
	
	// Check Delete the correct character
	uWBreak = gStringTryCount
	findDelChar = 0
	While(uWBreak>0)
		FindPic gDelCharCoordinate(0),gDelCharCoordinate(1),gDelCharCoordinate(2),gDelCharCoordinate(3),"Attachment:\DelChar.bmp",0.9,intX,intY
		If intX > 0 And intY > 0 Then
    		TracePrint "Found: DelChar.bmp."
    		uWBreak = 0
    		findDelChar = 1
    	Else 
    		TracePrint "Not Found: DelChar.bmp."
		End If
		Delay 25
		uWBreak = uWBreak - 1
	Wend
	
	If (findDelChar = 0) Then 
		TracePrint "Can't find the target character, exit the script"
		ExitScript
	End If
	
	// Delete Delay
	Delay 2000 * gDelayMultiple
	
	// Delete 2
	MoveTo gDeleteChar2XY(0), gDeleteChar2XY(1)
	Delay 25*gDelayMultiple
	LeftClick 1
