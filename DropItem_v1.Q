[Script]
//******** Windows
windowShift = array(100, 0)
hwnd = Plugin.Window.Find("Ragnarok", "Ragnarok")
Call Plugin.Window.Move(hwnd, 0+windowShift(0), 0+windowShift(1))

//******** User Parameters
UserVar About__ = DropList{"*********":1}=1 "** About "
UserVar gScriptAuthor = DropList{"Wade.Yeh":1}=1 "Author"
UserVar gScriptVersion = DropList{"20220815A":1}=1 "Version"
//**** Options
UserVar Options__ = DropList{"*********":1}=1 "** Options "
UserVar gDropItemNum = 2 "Drop n Items"

//******** Parameters
Dim cTrue
cTrue = 1
Dim cFalse
cFalse = 0

//** Coordinate
gBagPage3XY = array(620+windowShift(0), 970+windowShift(1))
gBagItem1XY = array(665+windowShift(0), 905+windowShift(1))
gBagItem2XY = array(695+windowShift(0), 905+windowShift(1))
gDropGroundXY = array(740+windowShift(0), 740+windowShift(1))

Function keyLoops(uKey, uLoops, uDelay)
	For uLoops
		Call Plugin.Bkgnd.KeyPress(hwnd, uKey)
		Delay uDelay
	Next
End Function

Function dropAtoB(uAX, uAY, uBX, uBY)
	MoveTo uAX, uAY
	Delay 250
	LeftDown 1
	Delay 250
	MoveTo uBX, uBY
	Delay 350
	LeftUp 1
	Delay 250
End Function

Function findCoordinateNum(uLUX, uLUY, uRDX, uRDY, uNum)
	Dim uWBreak
	Dim uTargetString
	Dim uResult
	uResult = cFalse
	uNum = CStr(uNum)
	uTargetString = "Attachment:\"+uNum+".bmp"
	
	uWBreak=gVCodeNumTryCount
	While(uWBreak>0)
		FindPic uLUX, uLUY, uRDX, uRDY, uTargetString, 1, intX, intY
		If intX > 0 And intY > 0 Then
    		TracePrint "Find: "+uNum+".bmp"
    		uWBreak = 0
    		uResult = cTrue
    	// Else 
    	//	TracePrint "Not Found: "+uNum+".bmp"
		End If
		Delay 25
		uWBreak = uWBreak - 1
	Wend
	findCoordinateNum = uResult
End Function

Function mainFunction()
	// Store Item * gAutoStoreNum
	For gDropItemNum
		Call dropAtoB(gBagItem1XY(0), gBagItem1XY(1), gDropGroundXY(0), gDropGroundXY(1))
		Call Plugin.Bkgnd.KeyPress(hwnd, 13)
		Delay 100
	Next
End Function


Call mainFunction()
